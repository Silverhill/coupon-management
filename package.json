    "@storybook/addon-actions": "^3.3.3",
    "@storybook/addon-links": "^3.3.3"
  },
  "ava": {
    "files": [
      "test/**/*.spec.js"
    ],
    "source": [
      "server/**/*.js"
    ],
    "failFast": true,
    "babel": "inherit",
    "require": [
      "babel-register",
      "babel-polyfill"
    ]
  }
