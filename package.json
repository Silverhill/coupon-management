  "dependencies": {
    "axios": "^0.17.1",
    "body-parser": "^1.18.2",
    "composable-middleware": "^0.3.0",
    "cross-env": "^5.1.1",
    "express": "^4.16.2",
    "express-jwt": "^5.3.0",
    "history": "^4.7.2",
    "mongoose": "^4.13.7",
    "passport": "^0.4.0",
    "passport-facebook": "^2.1.1",
    "passport-google-oauth": "^1.0.0",
    "passport-local": "^1.0.0",
    "path": "^0.12.7",
